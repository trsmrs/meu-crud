<!DOCTYPE html>
<html lang="pt-br">
<%- include('partials/head.ejs')%>

    <body>
        <%- include('partials/navbar.ejs')%>
            <h1>
                <%= title %>
            </h1>

            <ul>
                <% users.forEach((user)=>{ %>
                    <li>
                        <%= user.name %> - <%= user.email %> - Idade: <%= user.age %> -
                        <a href="/edit?id=<%=user._id%>">[editar]</a> 
                        <a href="/remove/<%=user._id%>">[remover]</a> 
                    </li>

                    <% }) %>
            </ul>
            <%- include('partials/footer.ejs')%>
    </body>

</html>